<div class="container">
<div style="text-align: center;"><img id="frontLogo"
        src="https://api.freelogodesign.org/files/12d3fe6a97424dd397f175ad3e6f39b8/thumb/logo_200x200.png?v=0" /></div>
<h1 style="text-align: center;">Research result</h1>

<div style="margin-top: 3%;">
{{#each businesses as |business|}}
<div class="card" style="width: 18rem; ">
    <div class="card-body">
        <h5 class="card-title" id="{{business.business_id}}">{{business.name}}</h5>
        <div>Rating: {{star_rating business.totalRating}}</div>
        <div>{{business.imageUrl}}</div>
        <p class="card-title" id="listDescription">{{business.description}}</p>

        <button><a href="/business/{{business.business_id}}">VIEW</a></button>
    </div>
</div>
{{/each}}
</div>
</div>